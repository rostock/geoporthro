    {{ form_label(form.configuration.height) }}{{ form_widget(form.configuration.height) }}
    <div class="clearContainer"></div>
    <div class="left">
        {{ form_widget(form.configuration.showOriginal) }}<label for="{{form.configuration.showOriginal.vars.id}}" class="labelCheck">{{ "mb.core.admin.featureinfo.label.showoriginal"|trans }}</label>
    </div>
    <div class="left">
        {{ form_widget(form.configuration.onlyValid) }}<label for="{{form.configuration.onlyValid.vars.id}}" class="labelCheck">{{ "mb.core.admin.featureinfo.label.onlyvalid"|trans }}</label>
    </div>
    <div class="clearContainer"></div>
