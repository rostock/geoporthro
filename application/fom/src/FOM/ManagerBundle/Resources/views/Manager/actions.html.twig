{% if actions is defined %}
    {% for k,v in actions %}
    {% if v.display is not defined or v.display != false %}
    {% set attr %}
    {% if v.attr is defined %}{% for k,v in v.attr %} {{k}}="{{v}}"{% endfor %}{% endif %}
    {% endset %}
    <a id="action-{{ k }}" href="{{ v.url }}"{{ attr }} title="{{ v.title | trans }}"></a>
    {% endif %}
    {% endfor %}
{% endif %}